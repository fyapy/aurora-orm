      "optional": true
    },
    "tsx": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "22.5.0",
    "@types/pg": "8.11.9",
    "pg": "8.12.0",
    "tsup": "8.2.4",
    "tsx": "4.19.1",
    "typescript": "4.9.4",
    "vitest": "2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fyapy/aurora-orm.git"
  }
